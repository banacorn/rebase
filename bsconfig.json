{
  "name" : "@glennsl/rebase",
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors"],
  "refmt": 3,
  "package-specs": [{
    "module": "commonjs",
    "in-source": true
  }],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "examples",
      "type": "dev"
    },
    {
      "dir": "__tests__",
      "type": "dev",
      "subdirs": true
    }
  ],
  "suffix": ".bs.js",
  "bs-dev-dependencies": ["bs-jest"]
}
